<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.LikeOrDislike" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MyApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:fontFamily">font/custom_font.xml</item>
        <item name="colorPrimary">#e06966</item>
        <item name="colorPrimaryVariant">#fc747c</item>
        <item name="colorOnPrimary">#ffffff</item>
        <item name="colorSecondary">#d0b8a6</item>
        <item name="colorSecondaryVariant">#44443c</item>
        <item name="colorOnSecondary">#ffffff</item>
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>


</resources>